{
  "name": "express-es6-rest-api",
  "version": "0.3.0",
  "description": "Starter project for an ES6 RESTful Express API",
  "main": "dist",
  "scripts": {
    "dev": "nodemon -w src --exec \"babel-node src --presets es2015,stage-0\"",
    "build": "babel src -s -D -d dist --presets es2015,stage-0",
    "start": "nodemon --exec babel-node src/index.js",
    "prestart": "npm run -s build",
    "test": "mocha --compilers js:babel-core/register --timeout 3000 --recursive  --require babel-polyfill --exit",
    "test:ut": "mocha --compilers js:babel-core/register --require babel-polyfill --exit",
    "test:nyc": "nyc npm test",
    "coverage": "nyc --reporter=html npm test"
  },
  "eslintConfig": {
    "extends": "eslint:recommended",
    "parserOptions": {
      "ecmaVersion": 7,
      "sourceType": "module"
    },
    "env": {
      "node": true
    },
    "rules": {
      "no-console": 0,
      "no-unused-vars": 1
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/developit/express-es6-rest-api.git"
  },
  "author": "Jason Miller <jason@developit.ca>",
  "license": "MIT",
  "dependencies": {
    "base64-img": "^1.0.3",
    "body-parser": "^1.13.3",
    "compression": "^1.5.2",
    "cors": "^2.7.1",
    "csvtojson": "^1.1.9",
    "del": "^3.0.0",
    "express": "^4.13.3",
    "fs.extra": "^1.3.2",
    "istanbul": "^0.4.5",
    "jsonwebtoken": "^8.1.0",
    "lodash": "^4.17.4",
    "md5": "^2.2.1",
    "moment": "^2.18.1",
    "mongoose": "^4.13.2",
    "mongoose-random": "^0.1.1",
    "morgan": "^1.8.2",
    "multer": "^1.3.0",
    "nodemailer": "^4.6.4",
    "nodemailer-smtp-transport": "^2.7.4",
    "password-hash": "^1.2.2",
    "rand-token": "^0.3.0",
    "resource-router-middleware": "^0.6.0",
    "sha1": "^1.1.1",
    "simple-encryptor": "^1.1.0",
    "slug": "^0.9.1",
    "uid": "0.0.2",
    "validator": "^8.2.0",
    "youtube-feeds": "^2.4.1",
    "youtube-info": "^1.2.0"
  },
  "devDependencies": {
    "babel-cli": "^6.9.0",
    "babel-core": "^6.9.0",
    "babel-plugin-transform-regenerator": "^6.26.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-stage-0": "^6.5.0",
    "chai-http": "^4.3.0",
    "mocha": "^4.0.1",
    "eslint": "^3.1.1",
    "chai": "^4.1.2",
    "nodemon": "^1.12.1",
    "nyc": "^11.3.0"
  },
  "bugs": {
    "url": "https://github.com/developit/express-es6-rest-api/issues"
  },
  "homepage": "https://github.com/developit/express-es6-rest-api#readme",
  "keywords": []
}
